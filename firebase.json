{
  "hosting": {
    "public": "build/web",  
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(js|css|html|json|ico|svg|png|jpg|jpeg|woff|woff2|ttf|eot)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          },
          {
            "key": "Expires",
            "value": "0"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "cafeteria2024-b355c",
          "appId": "1:419688568194:android:60c2eb51d9313ab99d2b9b",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "cafeteria2024-b355c",
          "configurations": {
            "android": "1:419688568194:android:60c2eb51d9313ab99d2b9b",
            "ios": "1:419688568194:ios:066584e26bfc14279d2b9b",
            "web": "1:419688568194:web:443b8e4bb7bb1d149d2b9b",
            "windows": "1:419688568194:web:ff1d6287821471949d2b9b"
          }
        }
      }
    }
  }
}
